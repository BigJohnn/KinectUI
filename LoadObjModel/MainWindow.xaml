<Window x:Class="LoadObjModel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="768" Width="1024">
      <!--  MouseMove="ViewPort3D_MouseMove"
        MouseLeftButtonDown="ViewPort3D_MouseLeftButtonDown"
        MouseWheel="ViewPort3D_MouseWheel"
        KeyDown="Window_KeyDown">-->
        <Grid x:Name="lay">
            <Viewport3D x:Name="vp" Margin="49.2,23.8,35.8,27.8">
                <Viewport3D.Camera>
                    <PerspectiveCamera FieldOfView="45" FarPlaneDistance="100" LookDirection="0,0,-3.4142135623731" NearPlaneDistance="0.1" Position="0,0,3.4142135623731" UpDirection="0,1,0"/>
                </Viewport3D.Camera>
            </Viewport3D>
            <Slider Height="23" HorizontalAlignment="Left" Margin="203,709,0,0" Name="slider1" VerticalAlignment="Top" Width="100" Maximum="100" />
            <Slider Height="23" HorizontalAlignment="Left" Margin="49,704,0,0" Name="slider2" VerticalAlignment="Top" Width="100" Maximum="100" />
            <Slider Height="23" HorizontalAlignment="Left" Margin="364,709,0,0" Name="slider3" VerticalAlignment="Top" Width="100" Minimum="-50" Maximum="2000" />
        </Grid>
</Window>
